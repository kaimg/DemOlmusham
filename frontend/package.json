{
  "name": "frontend",
  "private": true,
  "dependencies": {
    "axios": "^1.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.23.1",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start": "DANGEROUSLY_DISABLE_HOST_CHECK=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
